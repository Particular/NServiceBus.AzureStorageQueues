    <PackageReference Include="System.Spatial" Version="5.8.2" />
    <PackageReference Include="NServiceBus.AcceptanceTesting" Version="6.3.3" />
    <PackageReference Include="NUnit" Version="3.10.1" />
    <PackageReference Include="RhinoMocks" Version="3.6.1" />
    <PackageReference Include="Particular.CodeRules" Version="0.2.0" />
    <PackageReference Include="PublicApiGenerator" Version="7.0.1" />
