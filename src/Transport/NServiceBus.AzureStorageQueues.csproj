<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net452</TargetFramework>
    <AssemblyName>NServiceBus.Azure.Transports.WindowsAzureStorageQueues</AssemblyName>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>$(SolutionDir)NServiceBus.snk</AssemblyOriginatorKeyFile>
    <NoWarn>$(NoWarn);1591</NoWarn>
    <Description>NServiceBus Transport for the Windows Azure Storage Queues</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Janitor.Fody" Version="1.4.1" PrivateAssets="All" />
    <PackageReference Include="WindowsAzure.Storage" Version="[8.0.0, 9.0.0)" />
    <PackageReference Include="NServiceBus" Version="[6.0.0, 7.0.0)" />
    <PackageReference Include="Obsolete.Fody" Version="4.2.2" PrivateAssets="All" />
    <PackageReference Include="Fody" Version="2.1.0" PrivateAssets="All" />
    <PackageReference Include="Particular.CodeRules" Version="0.2.0" PrivateAssets="All" />
    <PackageReference Include="Particular.Packaging" Version="0.5.0" PrivateAssets="All" />
  </ItemGroup>

</Project>