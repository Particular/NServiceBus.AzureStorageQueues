<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net472;netcoreapp2.1</TargetFrameworks>
    <AssemblyName>NServiceBus.Transport.AzureStorageQueues</AssemblyName>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>$(SolutionDir)NServiceBus.snk</AssemblyOriginatorKeyFile>
    <RootNamespace>NServiceBus.AzureStorageQueues</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>NServiceBus.Transport.AzureStorageQueues</PackageId>
    <Description>NServiceBus Transport for Azure Storage Queues</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Storage.Queues" Version="[12.5.0, 13.0.0]" />
    <PackageReference Include="Azure.Storage.Blobs" Version="[12.7.0, 13.0.0]" />
    <PackageReference Include="Microsoft.Azure.Cosmos.Table" Version="[1.0.8, 2.0.0]" />
    <PackageReference Include="NServiceBus" Version="8.0.0-alpha.875" />
    <PackageReference Include="Fody" Version="6.3.0" PrivateAssets="All" />
    <PackageReference Include="Janitor.Fody" Version="1.8.0" PrivateAssets="All" />
    <PackageReference Include="Obsolete.Fody" Version="5.2.1" PrivateAssets="All" />
    <PackageReference Include="Particular.Packaging" Version="1.0.1" PrivateAssets="All" />
  </ItemGroup>

</Project>
